{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": "off",
        "import/no-extraneous-dependencies": "off",
        "import/no-unresolved": "off",
        "no-unused-vars": "off",
        "indent": "off",
        "no-underscore-dangle": "off",
        "no-console": "off",
        "import/prefer-default-export": "off"
    }
}
